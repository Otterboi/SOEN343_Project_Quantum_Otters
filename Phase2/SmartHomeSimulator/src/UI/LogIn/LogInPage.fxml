<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="loginPane" prefHeight="200.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.LogIn.FXMLDocumentController">
    <children>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <TextField fx:id="usernameField" layoutX="87.0" layoutY="68.0" promptText="Username" />
      <Label layoutX="144.0" layoutY="6.0" text="Log in" />
      <PasswordField fx:id="passwordField" layoutX="87.0" layoutY="116.0" promptText="Password" />
      <Label layoutX="87.0" layoutY="51.0" text="Username" />
      <Label layoutX="87.0" layoutY="100.0" text="Password" />
      <Button fx:id="logInButton" layoutX="87.0" layoutY="141.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Log In" />
      <Button fx:id="signUpButton" layoutX="177.0" layoutY="141.0" mnemonicParsing="false" onAction="#handleSignUpButton" text="Sign Up" />
